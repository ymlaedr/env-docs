# EditorConfig

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[{docker-compose}.yml]
indent_size = 2

[*.{js,json}]
indent_size = 2

[*.md]
indent_size = 2
trim_trailing_whitespace = false
insert_trailing_whitespace = true
